}

//packtest.java
package pack;
import p1.A;
import p1.B;
import p1.p11.C;
import p2.D;
class packtest{
public static void main(String args[])
{
A a1=new A();
B b1=new B();
C c1=new C();
D d1=new D();

a1.displayA();
b1.displayB();
c1.displayC();
d1.displayD();
}
}
